  gbLCCheckAxis = TRUE
  ;
  grLCTorqueMin = 350
  grLCTorqueMax = 1200
  grLCTorqueLim = 550
  grLCAccMin    = 1.22
  grLCAccMax    = 7
  ;
  grLCAccZero1  = 15.83
  grLCAccZero2  = 2.74 
  grLCAccDenom1 = -39.77
  grLCAccDenom2 = -790
