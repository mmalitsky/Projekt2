  grLCKv1       = 1
  grLCKv2       = 1
  grLCKv3       = 2
  grLCTLimKv21  = 700
  grLCTLimKv32  = 600
  ;	
  grLCLoadLimit   = 99 
  grLCLoadTimeLim = 0.8
  grLCStdValKV    = 1	
  grLCStdValAxAcc = 1.22
